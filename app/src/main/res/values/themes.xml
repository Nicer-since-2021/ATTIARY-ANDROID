<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Attiary" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/gray_button</item>
        <item name="colorPrimaryVariant">@color/yellow_box</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/yellow_top</item>
        <item name="colorSecondaryVariant">@color/yellow_box</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/yellow_background</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <!-- Customize your theme here. -->
        <item name="windowNoTitle">true</item>
    </style>

    <style name="CalendarDateTextStyle" parent="android:TextAppearance.DeviceDefault">
        <item name="android:fontFamily">@font/to_daughter_from_mom</item>
        <item name="android:textSize">35dp</item>
    </style>

    <style name="CalendarWeeksTextStyle" parent="android:TextAppearance.DeviceDefault">
        <item name="android:fontFamily">@font/im_hyemin_bold</item>
        <item name="android:textSize">16dp</item>
    </style>
</resources>